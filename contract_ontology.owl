    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#License -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#License">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#coversTerritory"/>
                <owl:someValuesFrom rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/Territory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#grantsRight"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/hasDuration"/>
                <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/Duration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/hasDuration"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.opertusmundi.eu/contract_ontology#DurationOfTheAgreement"/>
                            <rdf:Description rdf:about="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/ExplicitDuration"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <contract_ontology:todo>PW: Not sure whether we should model this as the legal document (definition 1 at https://en.wiktionary.org/wiki/license#Noun ) or as a collection of legal terms (definition 2 at https://en.wiktionary.org/wiki/license#Noun )</contract_ontology:todo>
    </owl:Class>
    


