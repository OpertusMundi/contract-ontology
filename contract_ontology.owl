        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#hasAccount"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioAccount"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
