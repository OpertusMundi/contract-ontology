<?xml version="1.0"?>
<rdf:RDF xmlns="http://https://www.opertusmundi.eu/contract_ontology#"
     xml:base="http://https://www.opertusmundi.eu/contract_ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/"
     xmlns:Analytics="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Analytics/"
     xmlns:contract_ontology="http://www.opertusmundi.eu/contract_ontology#"
     xmlns:AnnotationVocabulary="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"
     xmlns:SpecificationMetadata="http://www.omg.org/techprocess/ab/SpecificationMetadata/">
    <owl:Ontology rdf:about="http://www.opertusmundi.eu/contract_ontology">
        <owl:versionIRI rdf:resource="http://https://www.opertusmundi.eu/contract_ontology/0.1.0"/>
        <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/master/latest/FND/Agreements/Contracts/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.opertusmundi.eu/contract_ontology#todo -->

    <owl:AnnotationProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#todo"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.opertusmundi.eu/contract_ontology#belongsToInvoice -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#belongsToInvoice">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioInvoice"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioPayment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#coversTerritory -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#coversTerritory">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
        <rdfs:range rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/Territory"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#grantsRight -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#grantsRight">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#hasAccount -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#hasAccount">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/has"/>
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioUser"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioAccount"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#hasDeliveryDateTime -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#hasDeliveryDateTime">
        <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/hasDateTime"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#hasPassword -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#hasPassword">
        <rdfs:subPropertyOf rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/has"/>
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioAccount"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#Password"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#hasPayment -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#hasPayment">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioInvoice"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioPayment"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#isBasedOnContract -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#isBasedOnContract">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/Contract"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioInvoice"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#isDeliveredVia -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#isDeliveredVia">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioProduct"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioDeliveryMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#ledTo -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#ledTo">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioOrder"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/Contract"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#license -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#license">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/Contract"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#orderCustomer -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#orderCustomer">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioOrder"/>
        <rdfs:range rdf:resource="https://www.opertusmundi.eu/contract_ontology#Customer"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#orderItems -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#orderItems">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioOrder"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#OrderItemCollection"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#orderSupplier -->

    <owl:ObjectProperty rdf:about="http://www.opertusmundi.eu/contract_ontology#orderSupplier">
        <rdfs:domain rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioOrder"/>
        <rdfs:range rdf:resource="http://www.opertusmundi.eu/contract_ontology#Supplier"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://schema.org/Product -->

    <owl:Class rdf:about="http://schema.org/Product"/>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#DataProduct -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#DataProduct">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioProduct"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#ExclusiveLicense -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#ExclusiveLicense">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
        <owl:disjointWith rdf:resource="http://www.opertusmundi.eu/contract_ontology#NonExclusiveLicense"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#FileAccess -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#FileAccess">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioDeliveryMethod"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#Identifier -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#Identifier">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeElement"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#License -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#License">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#coversTerritory"/>
                <owl:someValuesFrom rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/Territory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#grantsRight"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/hasDuration"/>
                <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/Duration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#NonExclusiveLicense -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#NonExclusiveLicense">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#NonSublicenseableLicense -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#NonSublicenseableLicense">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
        <owl:disjointWith rdf:resource="http://www.opertusmundi.eu/contract_ontology#SublicenseableLicense"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#NonTransferableLicense -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#NonTransferableLicense">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
        <owl:disjointWith rdf:resource="http://www.opertusmundi.eu/contract_ontology#TransferableLicense"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#OnlineAccess -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#OnlineAccess">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioDeliveryMethod"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#OrderItem -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#OrderItem">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Arrangements/CollectionConstituent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#hasDeliveryDateTime"/>
                <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/DateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Arrangements/isConstituentOf"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#OrderItemCollection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/refersTo"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioProduct"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#OrderItemCollection -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#OrderItemCollection">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/Collection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Arrangements/hasConstituent"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.opertusmundi.eu/contract_ontology#OrderItem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#Password -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#Password">
        <rdfs:subClassOf rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/CodeElement"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#RightOfAdaption -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#RightOfAdaption">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
        <skos:definition xml:lang="en">The right of adaptation, arrangement, modification, correction of errors, and the right to transform, integrate, incorporate, in whole or in part and in any written, oral, telematic, digital, etc. form, all or part of the Products for the purpose of any type of exploitation.</skos:definition>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#RightOfTranslation -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#RightOfTranslation">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
        <skos:definition xml:lang="en">The right of translation, which includes the right to establish or have established any version, in any language, and in any computer language, of all or part of each of the Products.</skos:definition>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#RightToMarket -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#RightToMarket">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
        <skos:definition xml:lang="en">The right to market, grant or assign rights of use, to rent, to lend, to reproduce, to represent, exhibit, display, disseminate and exploit all or part of the Products in the original version or in any adapted, arranged, modified, corrected, transformed, abridged or extended version, or translated, whether in return for payment or free of charge by any means of communication to the public known to date and in particular by public recitation, by television broadcasting, including radio broadcasting, satellite transmission, initial or secondary, active or passive cable distribution, by public projection, by transmission in a public place, by digital transmission on line or on a support, by public presentation and any other means.</skos:definition>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#RightToPublish -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#RightToPublish">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
        <skos:definition xml:lang="en">The right to publish, disseminate, edit and republish, without limitation in quantity, including the right of reprography and the rights derived therefrom.</skos:definition>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#RightToReproduce -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#RightToReproduce">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#IntellectualPropertyRight"/>
        <skos:definition xml:lang="en">The right to reproduce without limitation in number, digitise, download, store, duplicate, print, record all or part of each of the Products, for the purposes covered by the License and in any manner whatsoever, in particular, by any technical process, on any media, in any format and via any tool, software, platform, website, mobile application or other system, known or unknown; the present right of reproduction including the right of permanent or temporary reproduction of any software in whole or in part, by any means and in any form, in particular for any loading, display, execution, transmission or storage operation.</skos:definition>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#Service -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#Service">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioProduct"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#SublicenseableLicense -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#SublicenseableLicense">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#License"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#Subscription -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#Subscription">
        <rdfs:subClassOf rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioProduct"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#Supplier -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#Supplier">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/ContractParty"/>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#TopioAccount -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#TopioAccount">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#hasPassword"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#Password"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/isIdentifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#Identifier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#TopioDeliveryMethod -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#TopioDeliveryMethod"/>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#TopioInvoice -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#TopioInvoice">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#isBasedOnContract"/>
                <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/Contract"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#hasPayment"/>
                <owl:allValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioPayment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#TopioOrder -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#TopioOrder">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#orderCustomer"/>
                <owl:someValuesFrom rdf:resource="https://www.opertusmundi.eu/contract_ontology#Customer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#orderItems"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#OrderItemCollection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#orderSupplier"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#Supplier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/hasEffectiveDate"/>
                <owl:someValuesFrom rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/Date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#ledTo"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/Contract"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#ledTo"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/Contract"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.opertusmundi.eu/contract_ontology#TopioPayment -->

    <owl:Class rdf:about="http://www.opertusmundi.eu/contract_ontology#TopioPayment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.opertusmundi.eu/contract_ontology#belongsToInvoice"/>
                <owl:someValuesFrom rdf:resource="http://www.opertusmundi.eu/contract_ontology#TopioInvoice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
